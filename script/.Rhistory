y <- 10L  # Assigning an integer value
class(y)
# Logical (Boolean values)
z <- TRUE  # Logical value
class(z)
# Character (String)
name <- "Alice"  # Assigning a string
class(name)
# Vectors (1D homogeneous data structure)
numbers <- c(1, 2, 3, 4, 5)  # Numeric vector
names <- c("Alice", "Bob", "Charlie")  # Character vector
# Accessing vector elements (1-based index)
numbers[1]  # First element
names[2:3]  # Second to third elements
# Lists (1D heterogeneous data structure)
my_list <- list("Alice", 25, TRUE)  # A list containing different data types
my_list[[1]]  # Accessing the first element
# Data Frames (2D table-like structure)
df <- data.frame(Name = c("Alice", "Bob"), Age = c(25, 30), Score = c(90, 85))
print(df)  # Print the data frame
df$Name  # Accessing a column
# Matrices (2D homogeneous data structure)
matrix_data <- matrix(1:9, nrow = 3, byrow = TRUE)  # 3x3 matrix
print(matrix_data)
matrix_data[2, 3]  # Access element at row 2, column 3
